{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true,
    "browser": true,
    "builtin": true
  },
  "globals": {
    "$": true,
    "$$": true,
    "React": true
  },
  "plugins": [
    "eslint-plugin-react",
    "eslint-plugin-random"
  ],
  "ecmaFeatures": {
    "jsx": true,
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 0,
    "comma-dangle": 0,
    "consistent-this": 2,
    "curly": [
      2,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "global-strict": [
      2,
      "never"
    ],
    "guard-for-in": 0,
    "max-len": [
      2,
      200,
      2
    ],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-nested-ternary": 2,
    "no-new": 0,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-with": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "radix": 2,
    "random/arrow-function-parens": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "space-after-keywords": 2,
    "space-before-blocks": 2,
    "space-in-brackets": [2, "never"],
    "space-return-throw-case": 2,
    "spaced-line-comment": 2,
    "strict": 0,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "react/display-name": 0,
    "react/jsx-boolean-value": 1,
    "react/jsx-quotes": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-sort-props": 0,
    "react/jsx-sort-prop-types": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-multi-comp": 0,
    "react/no-unknown-property": 1,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 1,
    "react/self-closing-comp": 1,
    "react/sort-comp": 0,
    "react/wrap-multilines": 0,
    "valid-jsdoc": [
      2,
      {
        "requireReturn": false,
        "prefer": {
          "return": "returns"
        }
      }
    ]
  }
}
